module util

imports
  base
  shared

signature
  sorts
    Lookup

  internal-constructors 
    lookup : String -> Lookup
  
  arrows
    Lookup -look-> Value 

rules // environment lookups

  E env |- lookup(x) -look-> v
  where
    env[x] => t,
    t != F(),
    S(t) => v
  
  E env |- lookup(x) -look-> v
  where
    all-fail,
    F() => v
